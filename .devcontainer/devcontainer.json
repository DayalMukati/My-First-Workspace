{
  "name": "n8n-codespace",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "settings": {},
      "extensions": [
        "dbaeumer.vscode-eslint"
      ]
    }
  },
  "forwardPorts": [5678],
  "postCreateCommand": "/init.sh",
  "remoteUser": "root",
  "mounts": [
    "source=${localWorkspaceFolder}/workflows,target=/workflows,type=bind"
  ]
}
